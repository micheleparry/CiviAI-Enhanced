# Oregon MCP Integration Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration for Oregon Planning
OREGON_DB_HOST=localhost
OREGON_DB_PORT=5432
OREGON_DB_NAME=civiai_enhanced
OREGON_DB_USER=postgres
OREGON_DB_PASSWORD=your_secure_password_here

# Oregon DLCD Monitoring Configuration
OREGON_DLCD_BASE_URL=https://www.oregon.gov/lcd
OREGON_MONITORING_ENABLED=true
OREGON_MONITORING_TIMEZONE=America/Los_Angeles

# File System Configuration
OREGON_PLANNING_ROOT=./oregon-planning
OREGON_DOCUMENT_MAX_SIZE=52428800
OREGON_BACKUP_ENABLED=true

# Notification Configuration
OREGON_NOTIFICATION_EMAIL_ENABLED=false
OREGON_NOTIFICATION_WEBHOOK_URL=
OREGON_NOTIFICATION_SLACK_WEBHOOK=

# Compliance Analysis Configuration
OREGON_AI_ANALYSIS_ENABLED=true
OREGON_COMPLIANCE_THRESHOLD=80
OREGON_AUTO_ANALYSIS_ENABLED=true

# Monitoring Schedules (Cron expressions)
OREGON_DLCD_MONITORING_SCHEDULE=0 9 * * *
OREGON_OAR_MONITORING_SCHEDULE=0 10 * * 1
OREGON_ORS_MONITORING_SCHEDULE=0 11 1 * *
OREGON_PAPA_MONITORING_SCHEDULE=0 8 * * *
OREGON_DEADLINE_CHECK_SCHEDULE=0 * * * *

# Security Configuration
OREGON_API_KEY=your_api_key_here
OREGON_ENCRYPTION_KEY=your_encryption_key_here
OREGON_SESSION_SECRET=your_session_secret_here

# Logging Configuration
OREGON_LOG_LEVEL=info
OREGON_LOG_FILE=./logs/oregon-mcp.log
OREGON_AUDIT_LOG_ENABLED=true

# Performance Configuration
OREGON_DB_POOL_SIZE=20
OREGON_DB_IDLE_TIMEOUT=30000
OREGON_DB_CONNECTION_TIMEOUT=2000
OREGON_CACHE_ENABLED=true
OREGON_CACHE_TTL=3600

# Integration Configuration
OREGON_WEB_SEARCH_API_KEY=your_web_search_api_key
OREGON_GITHUB_TOKEN=your_github_token
OREGON_GITHUB_REPO=your-org/oregon-planning-repo

# Development Configuration
OREGON_DEV_MODE=false
OREGON_MOCK_DATA_ENABLED=false
OREGON_TEST_MODE=false 